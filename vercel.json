{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "functions": {
    "api/claude-proxy.ts": {
      "runtime": "edge"
    }
  },
  "headers": [
    {
      "source": "/patterns.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, stale-while-revalidate=3600"
        }
      ]
    }
  ]
}
